{
    "name": "veepdotai/veepdotai",
    "require": {
        "orhanerday/open-ai": "^4.7",
        "paquettg/php-html-parser": "^3.1",
        "league/commonmark": "^2.4",
        "delight-im/str": "^2.20",
        "ginsen/img-finder": "^4.0",
        "inpsyde/wonolog": "^1.0",
        "stripe/stripe-php": "^13.3",
        "mustache/mustache": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "veepdotai\\veepdotai\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jean-Christophe Kermagoret",
            "email": "jck@veep.ai"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false,
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7"
    }
}
