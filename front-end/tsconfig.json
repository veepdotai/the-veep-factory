{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["*"],
      "@/constants/*": ["src/constants/*"],
      "@/components/ui/*": ["src/components/ui/shadcn/*"],
      "@/components/plate-ui/*": ["src/components/ui/shadcn/plate-ui/*"],
      "@/components/icons": ["src/components/ui/shadcn/plate-ui/icons.tsx"],
      "@/lib/utils": ["src/components/ui/shadcn/utils"],
      "@/hooks/*": ["src/components/ui/shadcn/hooks/*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "dist/types/**/*.ts",
    ".next/types/**/*.ts"
, "src/components/screens/PubTarget.js"  ],
  "exclude": [
    "node_modules"
  ]
}
